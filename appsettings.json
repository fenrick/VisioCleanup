{
  "Serilog": {
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "Sample"
    },
    "Using": ["Serilog.Sinks.Console"],
    "WriteTo": [
      {
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        },
        "Name": "Console"
      }
    ]
  },
  "VisioCleanupSettings": {
    "BottomPadding": 4,
    "LeftPadding": 4,
    "RightPadding": 4,
    "TopPadding": 12.5,
    "UltimateShapeHeight": 15,
    "UltimateShapeWidth": 40,
    "VisioHeightField": "Height",
    "VisioHorizontalSpacer": 4,
    "VisioLocPinXField": "LocPinX",
    "VisioLocPinYField": "LocPinY",
    "VisioPinXField": "PinX",
    "VisioPinYField": "PinY",
    "VisioUnits": "mm",
    "VisioVerticalSpacer": 2,
    "VisioWidthField": "Width",
    "ExcelHeaderFormat": "L{0}"
  }
}