{
  "Serilog": {
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "VisioCleanup"
    },
    "Using": ["Serilog.Sinks.Console"],
    "WriteTo": [
      {
        "Args": {
          "outputTemplate":
            "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:ljf} {Properties}{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        },
        "Name": "Console"
      }
    ]
  },
  "VisioCleanupSettings": {
    "BottomPadding": 4000,
    "ExcelFieldLabelFormat": "L{0}",
    "ExcelShapeTypeLabelFormat": "L{0} Shape",
    "ExcelSortFieldLabelFormat": "L{0} Sort",
    "HeaderHeight": 15000,
    "LeftPadding": 4000,
    "RightPadding": 4000,
    "SidePanelWidth": 15000,
    "TopPadding": 10000,
    "UltimateShapeHeight": 7500,
    "UltimateShapeWidth": 45000,
    "VisioHeightField": "Height",
    "VisioHorizontalSpacer": 4000,
    "VisioLocPinXField": "LocPinX",
    "VisioLocPinYField": "LocPinY",
    "VisioPinXField": "PinX",
    "VisioPinYField": "PinY",
    "VisioUnits": "mm",
    "VisioVerticalSpacer": 2000,
    "VisioWidthField": "Width"
  }
}