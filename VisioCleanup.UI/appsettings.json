{
  "Serilog": {
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "VisioCleanup"
    },
    "Using": ["Serilog.Sinks.WinForm"],
    "WriteTo": [
      {
        "Args": {
          "outputTemplate":
            "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:ljf} {Properties}{NewLine}{Exception}"
        },
        "Name": "WinForm"
      }
    ]
  },
  "VisioCleanup": {
    "Core": {
        "FieldLabelFormat": "L{0}",
        "ShapeTypeLabelFormat": "L{0} Shape",
        "SortFieldLabelFormat": "L{0} Sort",
        "Base": 4000,
        "Left": 4000,
        "Right": 4000,
        "Top": 10000,
        "HorizontalSpacing": 4000,
        "VerticalSpacing": 2000,
        "HeaderHeight": 15000,
        "SidePanelWidth": 15000,
        "Height": 7500,
        "Width": 45000,
        "HeightField": "Height",
        "LocPinXField": "LocPinX",
        "LocPinYField": "LocPinY",
        "PinXField": "PinX",
        "PinYField": "PinY",
        "Units": "mm",
        "WidthField": "Width"
    }
  }
}