{
  "Serilog": {
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "VisioCleanup"
    },
    "Using": ["Serilog.Sinks.Console"],
    "WriteTo": [
      {
        "Args": {
          "outputTemplate":
            "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:ljf} {Properties}{NewLine}{Exception}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        },
        "Name": "Console"
      }
    ]
  },
  "VisioCleanup": {
    "Core": {
      "Excel": {
        "FieldLabelFormat": "L{0}",
        "ShapeTypeLabelFormat": "L{0} Shape",
        "SortFieldLabelFormat": "L{0} Sort"
      },
      "Padding": {
        "Bottom": 4000,
        "Left": 4000,
        "Right": 4000,
        "Top": 10000,
        "HorizontalSpacing": 4000,
        "VerticalSpacing": 2000
      },
      "Page": {
        "HeaderHeight": 15000,
        "SidePanelWidth": 15000
      },
      "Defaults": {
        "Height": 7500,
        "Width": 45000
      },
      "VisioFields": {
        "HeightField": "Height",
        "LocPinXField": "LocPinX",
        "LocPinYField": "LocPinY",
        "PinXField": "PinX",
        "PinYField": "PinY",
        "Units": "mm",
        "WidthField": "Width"
      }
    }
  }
}