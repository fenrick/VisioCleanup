{
  "Serilog": {
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Properties": {
      "Application": "VisioCleanup"
    },
    "Using": [ "Serilog.Sinks.WinForm"],
    "WriteTo": [
        {
            "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:ljf} {Properties}{NewLine}{Exception}"
            },
            "Name": "WinForm"
        }
    ]
  },
  "VisioCleanup": {
    "Core": {
        "FieldLabelFormat": "L{0}",
        "ShapeTypeLabelFormat": "L{0} Shape",
        "SortFieldLabelFormat": "L{0} Sort",
        "Base": 4,
        "Left": 4,
        "Right": 4,
        "Top": 10,
        "HorizontalSpacing": 4,
        "VerticalSpacing": 2,
        "HeaderHeight": 15,
        "SidePanelWidth": 15,
        "Height": 12,
        "Width": 45
    }
  }
}